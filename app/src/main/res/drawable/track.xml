<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Black shadow. -->
    <item
        android:left="3dp"
        android:top="5dp">
        <shape
            android:dither="true"
            android:shape="rectangle"
            android:useLevel="false"
            android:visible="true">
            <solid android:color="#000000" />
            <corners android:radius="25dp" />
            <size
                android:width="2dp"
                android:height="18dp" />
        </shape>
    </item>
    <!-- Orange foreground. -->
    <item>
        <shape
            android:bottom="5dp"
            android:dither="true"
            android:right="3dp"
            android:shape="rectangle"
            android:useLevel="false"
            android:visible="true">
            <stroke
                android:width="2dp"
                android:color="@color/black" />
            <solid android:color="@color/white" />
            <corners android:radius="25dp" />
            <size
                android:width="2dp"
                android:height="18dp" />
        </shape>
    </item>
</layer-list>